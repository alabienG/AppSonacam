        <queryString language="SQL">
            <![CDATA[SELECT v.montant,
	v.numero,
	v.statut,
	concat( c.nom,' ',c.prenom) AS usager,
	z.libelle  as zones,
	v.date_save AS dates
FROM versement v
	inner join paiement p ON
	 v.paiement = p.id
	inner join facture f ON
	 p.facture = f.id
	inner join contribuable c ON
	 f.contribuable = c.id
	inner join zone z ON
	 c.zone = z.id]]>
        </queryString>
